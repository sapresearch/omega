<h1><%= @event.title %> Event's Details</h1>

<table>
  <tr>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Start Time</th>
    <th>End Time</th>
    <th>Event Description</th>

  </tr>
  <tr>
    <td><%= @event.start_date.to_s %> <%= @event.start_time.to_s(:time) %></td>
    <td><%= @event.end_date.to_s %> <%= @event.end_time.to_s(:time) %></td>


    <td><%= @event.event_description %></td>


  </tr>

</table>

<p>
  (<%= link_to('edit', :action => 'edit', :id => @event.id) %>)
</p>





