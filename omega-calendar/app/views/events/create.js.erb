$('#new_calendar_event').parents('.ui-dialog').andSelf().remove();
var cal = $('#cal').data('fullCalendar')
<% if @event.recurrent? %>
<% @event.recurrence_events.each do |event| %>
cal.renderEvent(<%= event.to_json.html_safe %>, true)
<% end %>

<% else %>
cal.renderEvent(<%= @event.to_json.html_safe %>, true)

<% end %>