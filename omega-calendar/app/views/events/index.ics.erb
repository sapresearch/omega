BEGIN:VCALENDAR
VERSION:2.0
<% @events.each do |event| %>
BEGIN:VEVENT
UID:<%= event.id %>
DTSTAMP:<%= event.created_at %>
DTSTART:<%= event.start %>
DTEND:<%= event.end %>
SUMMARY:<%= event.title %>
DESCRIPTION:<%= event.event_description %>
URL:<%= event.url %>
END:VEVENT
<% end %>
END:VCALENDAR
