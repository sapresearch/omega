<% if @service_registration %> 
  <% if @from_page == "services#index" %>
    <% if @registrant == current_contact %>
      $(".services_list").html("<%= escape_javascript( render "services/index_list" ) %>")
    <% end %>
  <% elsif @from_page == "service_registrations#index" %>
      $(".services_page_content").html("<%= escape_javascript( render "service_registrations/index_page_content" ) %>")    
  <% end %>
  $( "#service-registration-created-dialog-message" ).dialog({
      resizable: false,
      modal: true,
      buttons: {
          Ok: function() {
              $( this ).dialog( "close" );
          }
      },
      close: function() {
          $( "#service-registration-created-dialog-message" ).dialog('destroy')
      }
  });
<% else %>
  $( "#service-registration-create-failed-dialog-message" ).dialog({
      resizable: false,
      modal: true,
      buttons: {
          Ok: function() {
              $( this ).dialog( "close" );
          }
      },
      close: function() {
          $( "#service-registration-create-failed-dialog-message" ).dialog('destroy')
      }
  });
<% end %>

$(".tipsy").remove();
