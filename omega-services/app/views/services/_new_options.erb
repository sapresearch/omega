  <form>
    <label for="radio_service_level_leaf" >Service</label>
    <input id="radio_service_level_leaf" name="service_level" type="radio" checked="checked"
           onclick="$.ajax({ url:'<%= new_service_url %>.js', data:({service_level:'leaf'}) });" />
    <label for="radio_service_level_inner" >Service Category</label>
    <input id="radio_service_level_inner" name="service_level" type="radio"
           onclick="$.ajax({ url:'<%= new_service_url %>.js', data:({service_level:'inner'}) });" />
  </form>
