$('#admin-edit-em').empty().append('<%= escape_javascript( render :partial => "form_builder/partials/properties_input.html") %>').effect('highlight');

        var preview = $('#fields_<%="#{@em_id}"%>_field_name_preview');
        var field_name_submit = $('#fields_<%="#{@em_id}"%>_field_name');
        var field_value_preview = $("#<%="value-preview-#{@em_id}"%>");
        var field_value_submit = $('#fields_<%="#{@em_id}"%>_field_type');

        $('#edit-field-value').change(function() {
            var v = $(this).val();
            field_value_preview.find('input:disabled').val(v);
            field_value_submit.val(v);

        });
        $('#edit-label-value').keyup(function() {

            var v = $(this).val();
            preview.empty().append(v);
            field_name_submit.val(v)

        });
