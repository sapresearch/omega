$('#admin-edit-em').empty().append('<%= escape_javascript( render :partial => "form_builder/partials/properties_input.html") %>').effect('highlight');

        $('#edit-field-value').change(function() {
            var v = $(this).val();
            field_value_preview.find('input:disabled').val(v);
            field_value_submit.val(v);

        });
        $('#edit-label-value').keyup(function() {

            var v = $(this).val();
            $('#fields_<%="#{@em_id}"%>_field_name_preview').empty().append(v);
            $('#fields_<%="#{@em_id}"%>_field_type').val(v)

        });
$('#edit-db-value').keyup(function() {

            var v = $(this).val();

            $('#db-value-preview-<%="#{@em_id}"%>').val(v);

        });
