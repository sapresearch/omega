$('#admin-edit-em').empty().append('<%= escape_javascript( render :partial => "form_builder/partials/registration/properties_input.html") %>').effect('highlight');


        $('#edit-label-value').keyup(function() {

            var v = $(this).val();
            $('#fields_<%="#{@em_id}"%>_field_name_preview').empty().append(v);

