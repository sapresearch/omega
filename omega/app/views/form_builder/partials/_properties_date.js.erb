$('#admin-edit-em').empty().append('<%= escape_javascript( render :partial => "form_builder/partials/properties_date.html") %>').effect('highlight');

        $('#ui-em-<%="#{@em_id}"%>').append('<input type="hidden" value="Service Details" name="fields[<%="#{@em_id}"%>][field_category]" >');
        var preview = $('#fields_<%="#{@em_id}"%>_field_name_preview');
        var field_name_submit = $('#fields_<%="#{@em_id}"%>_field_name');
        var field_value_preview = $("#<%="value-preview-#{@em_id}"%>");
        var field_value_submit = $('#fields_<%="#{@em_id}"%>_field_type');

        $('#edit-label-value').keyup(function() {

            var v = $(this).val();
            preview.empty().append(v);
            field_name_submit.val(v)

        });


        $('.datepicker').datepicker({
            onSelect: function(dateText, inst) {
            field_value_preview.empty().append(dateText);
            field_value_submit.val(dateText);
            }

        });
