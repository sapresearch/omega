$('#admin-edit-em').empty().append('<%= escape_javascript( render :partial => "form_builder/partials/properties_textarea.html") %>').effect('highlight');
        $('#ui-em-<%="#{@em_id}"%>').append('<input type="hidden" value="Service Details" name="fields[<%="#{@em_id}"%>][field_category]" >');

        $('#edit-field-value').keyup(function() {
            var v = $(this).val();
            $("#<%="value-preview-#{@em_id}"%>").empty().append(v);
            $('#fields_<%="#{@em_id}"%>_field_name').val(v)

        });
        $('#edit-label-value').keyup(function() {
            var v = $(this).val();
            $('#fields_<%="#{@em_id}"%>_field_name_preview').empty().append(v);
            $('#fields_<%="#{@em_id}"%>_detail_attributes_field_value').val(v)
        });
