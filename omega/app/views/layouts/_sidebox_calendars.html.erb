<div id="sidebar_cal">
<h1>Your calendars </h1>
<div>
  <span class="btn_action">  <%= link_to('New Calendar', new_calendar_path) %></span>

</div>



<ul>
   <%  @calendars.each do |calendar| %>
        <li>
          <%= link_to calendar.calendar_name, calendar , {:remote => true,   :class => 'corners' + " " +cycle('o_08D9CD','o_023431','o_0AF3E7','o_07B3AA','o_05736E') }  %>
          <%= link_to 'edit', edit_calendar_path(calendar), :class => 'right' %>
        </li>

  <% end %>

</ul>


</div>