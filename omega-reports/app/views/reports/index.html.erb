<% require_javascript "swfobject" %>

<div style="margin:15 180">
  <%= @graph.html_safe %>
</div>


<% require_javascript "Highcharts-2.1.4/js/highcharts.js" %>
<% require_javascript "Highcharts-2.1.4/js/themes/gray.js" %>

<div id="chart-container-1" style="width: 100%; height: 400px"></div>
<script type="text/javascript">
var chart1; // globally available
$(document).ready(function() {
      chart1 = new Highcharts.Chart({
         chart: {
            renderTo: 'chart-container-1',
            defaultSeriesType: 'bar'
         },
         title: {
            text: 'Position Hours Overview'
         },
         xAxis: {
            categories: ['ESL teacher', 'Position2', 'Position3']
         },
         yAxis: {
            title: {
               text: 'Working Hours'
            }
         },
         series: [{
            name: 'Jane',
            data: [1, 0, 4]
         }, {
            name: 'John',
            data: [5, 7, 3]
         }]
      });
   });
</script>
