<% if @position.skills %>
    <%= side_bar_box(:skills) do %>
        <% @position.skills.each do |skill| %>
            <span class="skill-tag">
             <%= link_to skills_volunteering_positions_path(:skills => [params[:skills], skill.name].compact.join('+')) do %>
                <%= skill.name %>
            <% end %>
           </span>
        <% end %>
    <% end %>

<% end %>