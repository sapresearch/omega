<ul id="volunteers_list" style="margin-left 10px;">
<% @filter.each do |row| %>
	<% user = Contact.find(row[:id]).user %>
	<% if not user.nil? %>
		<% unless row[:first_name].to_s.empty? and row[:last_name].to_s.empty? %>
			<br /><br />
			<div class="volunteers_list_item">
				<li id="contact" ui-data="<%= row[:id] %>" >
					<span class="om-icon-only om-blue-icon ui-icon-plus" data-tooltip="Add this volunteer"></span>
					<%= " #{row[:first_name].to_s} #{row[:last_name].to_s}" %>
					<%#= link_to " #{row[:first_name].to_s} #{row[:last_name].to_s}", my_page_user_path(Contact.find(row[:id]).user) %>
				</li>
		  		<%= om_button(:contact => true, :icon => 'user-add') do %>
					<%= link_to "#{row[:first_name]}'s MyOmega", my_page_user_path(Contact.find(row[:id]).user), :'data-tooltip' => "See this user's homepage" %>
		  		<%end%>
			</div>
		<% end %>
	<% end %>
<% end %>
</ul>
