<% stylesheet_link_tag 'uploadify/uploadify' %>
<% javascript_include_tag 'uploadify/swfobject', 'uploadify/jquery.uploadify.v2.1.0' %>

<%= form_for @contact, :html => { :multipart => true } do |f| %>
    <%= errors_for_form @contact %>

    <%= f.fields_for :uploads, [Upload.new] do |u| %>
        <%= u.label :upload %>
        <%= u.file_field :upload %>
    <% end %>
<% end %>
