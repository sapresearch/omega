<fieldset class="round bg_eee">
  <legend class="round shadow">New account</legend>
  <%= render 'form' %>
</fieldset>
  
<div class="accounts_bottom_links page_bottom_links" >
  <div class="left_links">
    <%= link_to_function 'Create', '$(".new_account").submit()', :class=>"operation_button" %>
  </div>
  <div class="right_links">
    <%= link_to 'Back', accounts_path, :class=>"operation_button" %>
  </div>
</div>

<script type="text/javascript">
  $('.operation_button').button({ icons: {primary:'ui-icon-gear'} });
</script>
