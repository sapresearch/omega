
        <% unless record.contact.nil?%>
        <tr id="record-<%= record.id %>">

          <td>

             <%= om_button(:plain => true, :icon => 'next') do %>
                <%=  link_to 'See Application', tenant_volunteering_record_path(record) %>
            <% end %>
          </td>
          <td>
            <%= om_button(:plain => true, :icon => 'user') do %>
                <%=  link_to "#{record.contact.first_name} #{record.contact.last_name}", tenant_contact_path(record.contact), :class => 'blue' %>
            <% end %>
          </td>
          <td><%= om_button(:plain => true, :icon => 'mail-sent') do %>
                <span>  <%= record.created_at.strftime("%m/%d/%Y at %I:%M%p") %></span>
            <% end %>
          </td>
          <td>
            <%= om_button(:plain => true, :icon => 'folder-open') do %>
                <%=  link_to record.action , tenant_administer_volunteering_record_url(record), :remote => 'true' %>
            <% end %>
          </td>
          <td> <%= om_button(:plain => true, :icon => 'folder-open') do %>
                <%= record.status %>
            <% end %>
          </td>



        </tr>
        

        <%end%>
