<tr class="<%= cycle('even','odd') %>">
  <td>
    <%= d.object.day %>
    <%= d.hidden_field :day %>
  </td>
  <td>
    <%= d.check_box :enabled, :id => nil %>

  </td>
  <td>
    <%= d.text_field :start_time, :disabled => true, :size=> 8, :class=> 'tpickr' %>
  </td>
  <td>
    <%= d.text_field :end_time, :disabled => true, :size=> 8, :class=> 'tpickr' %>
  </td>
  <td>
    <%= d.text_field :volunteers, :disabled => true, :size=> 5 %>
  </td>
</tr>




