<% if @group_request %>
  $(".groups_list").html("<%= escape_javascript( render "groups/index_list" ) %>")
  show_dialog_message("group-request-created-dialog-message")
<% elsif @group.is_blocked? %>
  show_dialog_message("group-request-create-blocked-dialog-message")
<% elsif not @eligible %>
  load_dialog_message("<%= escape_javascript(render "groups_requesters/dialog_create_not_eligible_message", :group=>@group) %>")
<% elsif %>
  show_dialog_message("group-request-create-duplicate-dialog-message")
<% end %>

$(".tipsy").remove();