<%= side_bar_box(:Members) do %>
    <ul id="sidebar-member-list">
    <% @users.each do |user| %>
       <li>
         <%= om_button(:plain => true, :icon => 'user') do %>
              <%= link_to user.username , user_url(user) %>
          <% end %>
        </li>
    <% end %>
    </ul>

<% end %>