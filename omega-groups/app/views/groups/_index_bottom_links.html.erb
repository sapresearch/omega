<div class="left_links">
  <% if is_admin?(current_user) %>
    <%= link_to_function "New", "new_group('#{escape_javascript(render "groups/dialog_new_form", :group=>@new_group)}')", :class=>"operation_button" %>
  <% end %>
</div>
<div class="right_links">
  <% unless @super_group.nil? %>
    <%= link_to "Back to "+(@super_group.is_root? ? "All Groups" : @super_group.super_group.name),
      groups_url(:group_id=>@super_group.id), :class=>"operation_button", :remote=>true %>
  <% end %>
</div>
<script type="text/javascript">
  $('.operation_button').button({ icons: {primary:'ui-icon-gear'} });
</script>